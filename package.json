{
  "name": "video-react",
  "type": "module",
  "version": "0.17.0",
  "description": "Video-React is a web video player built from the ground up for an HTML5 world using React library.",
  "main": "lib/index.js",
  "style": "dist/video-react.css",
  "scripts": {
    "test": "BABEL_ENV=test jest",
    "test:watch": "BABEL_ENV=test jest --watch",
    "test:coverage": "BABEL_ENV=test jest --coverage",
    "start": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" webpack-dev-server --watch",
    "build-docs": "cross-env WEBPACK_BUILD=production NODE_OPTIONS=\"--openssl-legacy-provider\" webpack --progress --colors",
    "build": "rollup -c",
    "prebuild": "cross-env BABEL_ENV=lib-dir babel src --out-dir lib --ignore src/__tests__/",
    "create-release": "npm test && sh ./scripts/release",
    "create-major": "sh ./scripts/release major",
    "create-minor": "sh ./scripts/release minor",
    "create-patch": "sh ./scripts/release patch",
    "publish-release": "npm test && sh ./scripts/publish",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,css,md}": [
      "npm run format-one",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/video-react/video-react.git"
  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lib",
    "dist",
    "styles",
    "tonic-example.js"
  ],
  "keywords": [
    "react",
    "video",
    "video-react",
    "react-video",
    "player",
    "component",
    "components",
    "react-component",
    "react-components",
    "react component",
    "react components",
    "ui"
  ],
  "contributors": [
    "Junmin Liu",
    "Mengdi Chen",
    "Tianxiao Wang",
    "Xincun Li",
    "Jing Sun",
    "Xiaolei Chen",
    "Xiangkun Yang"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/video-react/video-react/issues"
  },
  "homepage": "https://github.com/video-react/video-react#readme",
  "tonicExampleFilename": "tonic-example.js",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "classnames": "^2.5.1",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@biomejs/biome": "1.9.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "conventional-recommended-bump": "^10.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "deep-freeze": "0.0.1",
    "ejs": "^3.1.10",
    "file-loader": "^6.2.0",
    "history": "^5.3.0",
    "hls.js": "^1.5.15",
    "holderjs": "^2.9.9",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^15.2.10",
    "mini-css-extract-plugin": "^2.9.1",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-prism": "^4.3.2",
    "react-router": "^6.26.2",
    "reactstrap": "^9.2.2",
    "rollup": "^4.22.4",
    "rollup-plugin-sass": "^1.13.2",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "sinon": "^19.0.2",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "style-loader": "^4.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
